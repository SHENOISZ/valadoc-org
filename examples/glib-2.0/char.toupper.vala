public static int main (string[] args) {
	// Output: ``AA1 !``
	stdout.printf ("%c", 'A'.toupper ());
	stdout.printf ("%c", 'a'.toupper ());
	stdout.printf ("%c", '1'.toupper ());
	stdout.printf ("%c", ' '.toupper ());
	stdout.printf ("%c", '!'.toupper ());
	stdout.printf ("%c", '\n'.toupper ());
	return 0;
}
