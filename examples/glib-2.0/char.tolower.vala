public static int main (string[] args) {
	// Output: ``aa1 !``
	stdout.printf ("%c", 'A'.tolower ());
	stdout.printf ("%c", 'a'.tolower ());
	stdout.printf ("%c", '1'.tolower ());
	stdout.printf ("%c", ' '.tolower ());
	stdout.printf ("%c", '!'.tolower ());
	stdout.printf ("%c", '\n'.tolower ());
	return 0;
}
